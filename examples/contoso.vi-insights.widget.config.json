{
    "$schema": "https://api.videoindexer.ai/schema/widgets/insights",
    "dataSources": {
        "index": {
            "dataSchemaVersion": "1.0",
            "init_language": "en-US",
            "templateUrl": "https://contoso.auzrewebsites.net/indexes/${accountId}/${videoId}/${language}",
            "httpHeaders": {
                "authorization": "Bearer 123"
            },
            "translations": ["en-US","de-DE"]
        },
        "peopleSprite": {
            "templateUri": "https://contoso.auzrewebsites.net/indexes/${accountId}/$videoId/sprites/people/${page}",
            "page_size": 50
        },      
        "keyframesSprite": {
            "template_uri": "https://contoso.auzrewebsites.net/indexes/${accountId}/$videoId/sprites/kf/${page}",
            "page_size": 50
        }       
    },
    "downloadOptions": ["json", "vtt", "zip", "video"],
    "preset": "Storyboard",
    "widgets": ["people", "emotions", "keywords"],
    "tab": "timeline",
    "locale": "de-DE"
}